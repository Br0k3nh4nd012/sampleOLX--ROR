<%= form_with url: '/profile/edit' ,model: profile do |form| %>
    <% if profile.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(profile.errors.count, "error") %> prohibited this product from being saved:</h2>
  
        <ul>
          <% profile.errors.each do |error| %>
            <li><%= error.full_message %></li>
          <% end %>
        </ul>
      </div>
    <% end %>
    <div class="field">
      <%= form.label :name %>
      <%= form.text_field :name %>
    </div>
    <div class="field">
      <%= form.label :email%>
      <%= form.text_field :email %>
    </div>
    <div class="field">
      <%= form.label :mobNumber , "Mobile Number"%>
      <%= form.text_field :mobNumber %>
    </div>
    <div class="field">
      <%= form.label :address %>
      <%= form.text_area :address %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
